{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "jsonata",
      "description": "workflows prek",
      "fileFormat": "toml",
      "managerFilePatterns": [
        "/(^|/)\\.github/workflows/.*\\.ya?ml$/"
      ],
      "matchStrings": [
        "$map(,  jobs.*.steps[$match(uses, /^j178/prek-action@.*/)].with.`prek-version`,,  function($version) {,    {,      \"depName\": \"prek\",,      \"datasource\": \"crate\",,      \"version\": $version,    },  },)"
      ],
      "versioningTemplate": "semver"
    }
  ],
  "enabled": true
}
